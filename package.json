{
  "name": "phologix",
  "version": "1.0.0",
  "description": "AI powered local album",
  "main": "./out/main/index.js",
  "author": "phologix.unv.app",
  "homepage": "https://phologix.unv.app",
  "scripts": {
    "rebuild": "electron-rebuild",
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "chcp 65001 && electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:win:full": "npm run build && electron-builder --win --config electron-builder.full.win.yml",
    "build:win:full:cu121": "npm run build && electron-builder --win --config electron-builder.full.win.cu121.yml",
    "build:win:full:ollama": "npm run build && electron-builder --win --config electron-builder.full.win.ollama.yml",
    "build:win:full:cu121:ollama": "npm run build && electron-builder --win --config electron-builder.full.win.cu121.ollama.yml",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@electron/rebuild": "^3.7.1",
    "@tailwindcss/vite": "^4.0.17",
    "@types/shelljs": "^0.8.15",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.9.1",
    "chokidar": "^4.0.3",
    "electron-updater": "^6.3.9",
    "element-plus": "^2.9.7",
    "js-md5": "^0.8.3",
    "meilisearch": "^0.49.0",
    "ollama": "^0.5.14",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "shelljs": "^0.9.2",
    "tailwindcss": "^4.0.17",
    "tree-kill": "^1.2.2",
    "typeorm": "^0.3.21",
    "vue-router": "4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.2"
  }
}
